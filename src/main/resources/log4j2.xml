<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="[%-5p] %d %c: %m [%t] %3x%n" /><!-- %d %-5p %C{2} (%F:%L) - %m%n -->
		</Console>
	</Appenders>

	<Loggers>
		<Root level="debug">
			<AppenderRef ref="console" />
		</Root>
	</Loggers>
</Configuration>